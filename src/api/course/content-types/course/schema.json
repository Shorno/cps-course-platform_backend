{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    },
    "overview": {
      "type": "blocks",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "durationWeeks": {
      "type": "integer"
    },
    "totalClasses": {
      "type": "integer"
    },
    "totalContests": {
      "type": "integer"
    },
    "totalProblems": {
      "type": "integer"
    },
    "requirements": {
      "type": "blocks"
    },
    "audience": {
      "type": "blocks"
    },
    "modules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::module.module"
    }
  }
}
